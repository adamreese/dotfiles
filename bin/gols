#!/usr/bin/env bash
set -euo pipefail
IFS=$'\n\t'

item=${1:-imports}

package="${PWD#$GOPATH/src/}"

not_stdlib() {
  xargs go list -f '{{if not .Standard}}{{.ImportPath}}{{end}}' || :
}

while read -r pkg; do
  echo "${pkg#${package}/vendor/}"
done < <(go list -f '{{join .'${item^}' "\n"}}' ./... | sort -u | not_stdlib)
