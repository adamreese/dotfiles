{
  "DisableAll": true,
  "Enable": [
    "deadcode",
    "dupl",
    "errcheck",
    "goconst",
    "gocyclo",
    "goimports",
    "gosimple",
    "ineffassign",
    "interfacer",
    "misspell",
    "staticcheck",
    "structcheck",
    "unconvert",
    "unparam",
    "unused",
    "varcheck",
    "vet",
    "vetshadow"
  ],
  "Exclude": [
    "error return value not checked.*(Close|Log|Print|Encode).*\\(errcheck\\)$",
    ".*_test\\.go:.*error return value not checked.*\\(errcheck\\)$",
    ".*\\.pb\\.go"
  ],
  "Vendor": true,
  "Deadline": "2m",
  "Concurrency": 4
}
