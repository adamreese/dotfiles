{
  "DisableAll": true,
  "Sort": [
    "linter",
    "severity",
    "path",
    "line"
  ],
  "WarnUnmatchedDirective": true,
  "Enable": [
    "deadcode",
    "goconst",
    "goimports",
    "ineffassign",
    "interfacer",
    "megacheck",
    "misspell",
    "structcheck",
    "unconvert",
    "unparam",
    "varcheck",
    "vet"
  ],
  "EnableGC": true,
  "Exclude": [
    "error return value not checked.*(Close|Log|Print|Encode).*\\(errcheck\\)$",
    ".*_test\\.go:.*error return value not checked.*\\(errcheck\\)$",
    ".*\\.pb\\.go"
  ],
  "Vendor": true,
  "Deadline": "2m",
  "Concurrency": 4
}
